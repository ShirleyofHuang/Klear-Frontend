{
  "expo": {
    "name": "klearFrontend",
    "slug": "klearFrontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/klear_logo.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shirleyofhuang.KlearFrontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/klear_logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.shirleyofhuang.KlearFrontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "dcsil",
            "project": "klear_frontend",
            "authToken": ""
          }
        }
      ]
    },
    "plugins": [
      "sentry-expo"
    ],
    "extra": {
      "SENTRY_DNS": "",
      "eas": {
        "projectId": "bcf86e71-959d-49d3-b626-5bbda23e44de"
      }
    }
  }
}
